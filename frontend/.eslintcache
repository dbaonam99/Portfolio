cursor.current.style.left = event.pageX + 'px';\n                    }} >  \n                    <div \n                        className={classNames('cursor', {\n                            cursor_hover: hover\n                        })}\n                        ref={cursor} \n                    ></div>  \n\n                    { tabChange &&\n                        <LoadingChildOut/>\n                    }\n                    { showFl &&\n                        <Follow/>\n                    }\n                    <Header/>\n                    <Route path=\"/\" exact component={Home}></Route>\n                    <Route path=\"/home\" exact component={Home}></Route>\n                    <Route path=\"/about\" exact component={About}></Route> \n                    <Route path=\"/project\" exact component={Portfolio}></Route>\n                    <Route path=\"/contact\" exact component={Contact}></Route>\n                </div>\n            }\n        </Router>\n    )\n}","/Users/namduong/Documents/dev/portfolio-project/frontend/src/components/Layouts/LoadingPage.js",["76"],"import React, { useContext, useEffect } from 'react';\nimport '../../Styles/LoadingPage.css' \nimport Div100vh from 'react-div-100vh';  \nimport TypeWriterEffect from 'react-typewriter-effect'; \nimport { LoadingContext } from '../../Contexts/Loading';\n \nexport default function LoadingPage() { \n    \n    const {\n        loading,\n        setLoadingFunc\n    } = useContext(LoadingContext) \n\n    useEffect(()=>{\n        if (window.location.href.split('/')[3] === \"\") {\n            document.body.style.overflow = 'hidden';\n            setTimeout(()=>{\n                setLoadingFunc(false) \n                document.body.style.overflow = 'unset';\n            }, 3000)\n        } else {\n            setLoadingFunc(false)\n        }\n    },[]) \n\n    return(\n        <Div100vh className={loading ? \"loading-page show-loading\" : \"loading-page\"}>  \n            <div className=\"loadi